<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="337.0" prefWidth="376.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddTaskController">
   <children>
      <Label layoutX="111.0" layoutY="23.0" text="Add Task">
         <font>
            <Font size="37.0" />
         </font>
      </Label>
      <TextField fx:id="txtTaskName" layoutX="36.0" layoutY="119.0" prefHeight="25.0" prefWidth="150.0" promptText="Task Name">
         <font>
            <Font size="17.0" />
         </font>
      </TextField>
      <TextField fx:id="txtTaskDescription" layoutX="36.0" layoutY="180.0" prefHeight="38.0" prefWidth="246.0" promptText="Task Description">
         <font>
            <Font size="17.0" />
         </font>
      </TextField>
      <Button fx:id="btnSubmit" layoutX="66.0" layoutY="246.0" mnemonicParsing="false" onAction="#SubmitTask" prefHeight="25.0" prefWidth="90.0" text="Submit">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="216.0" layoutY="246.0" mnemonicParsing="false" onAction="#Cancel" prefHeight="38.0" prefWidth="90.0" text="Cancel">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
