<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="305.0" prefWidth="451.0" stylesheets="@../font.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddTaskController">
   <children>
      <Label layoutX="129.0" layoutY="14.0" text="Add Task">
         <font>
            <Font size="37.0" />
         </font>
      </Label>
      <TextField fx:id="txtTaskDescription" layoutX="171.0" layoutY="112.0" prefHeight="30.0" prefWidth="246.0" promptText="Task Description">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="btnSubmit" layoutX="84.0" layoutY="231.0" mnemonicParsing="false" onAction="#SubmitTask" prefHeight="25.0" prefWidth="90.0" text="Submit" />
      <Button fx:id="btnCancel" layoutX="204.0" layoutY="231.0" mnemonicParsing="false" onAction="#Cancel" prefHeight="25.0" prefWidth="90.0" text="Cancel" />
      <Label layoutX="26.0" layoutY="112.0" text="Task Description">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
